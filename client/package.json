{
  "name": "issues-client",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --outfile=build/bundle.js",
    "watch": "esbuild src/index.tsx --bundle --outfile=build/bundle.js --watch",
    "build-prod": "esbuild src/index.tsx --bundle --minify --outfile=build/bundle.js",
    "reinstall": "rm -rf node_modules && npm install",
    "ci-check": "npm run lint && npm run tsc && npm run test",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-router-dom": "5.3.3",
    "axios": "0.26.1",
    "bson": "^4.6.5",
    "ramda": "^0.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-edit-text": "^5.0.2",
    "react-markdown": "^8.0.3",
    "react-modal": "^3.15.1",
    "react-router-dom": "6.3.0",
    "remark-gfm": "^3.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/ramda": "^0.28.15",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "esbuild": "^0.15.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
